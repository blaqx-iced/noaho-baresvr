{
	"compilerOptions": {
		"outDir": "dist",
		"lib": ["ESNext"],
		"target": "ESNext",
		"module": "Node16",
		"moduleResolution": "Node16",
		"strict": false,
		"stripInternal": true,
		"esModuleInterop": true,
		"declaration": true,
		"sourceMap": true,
		"resolveJsonModule": false,
		"skipLibCheck": true
	},
	"include": ["src"]
}
